{
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "include": "./out/test/fixture",
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "template": "./out/src/",
    "destination": "./out/test/jsdoc",
    "recurse": true,
    "readme": "./out/test/fixture/README.md",
    "package": "./out/test/fixture/package.json",
    "private": true
  },
  "cloudy": {
    "title": "sample for test",
    "description": "this is sample for test description.",
    "styles": ["./out/test/fixture/static/style1.css", "./out/test/fixture/static/style2.css"],
    "scripts": ["./out/test/fixture/static/script1.js", "./out/test/fixture/static/script2.js"]
  }
}
